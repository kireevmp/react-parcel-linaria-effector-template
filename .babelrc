{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": "commonjs",
        "corejs": 2,
        "targets": {
          "browsers": "last 2 versions"
        },
        "useBuiltIns": "entry"
      }
    ],
    "@babel/react",
    "@babel/typescript",
    "linaria/babel"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 2
      }
    ]
  ]
}
